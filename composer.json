{
    "name": "itcaretcr/landid-icons",
    "description": "A package to easily make use of Heroicons in your Laravel Blade views.",
    "keywords": ["Blade", "Heroicons", "Laravel"],
    "homepage": "https://github.com/ITCareTCR/pp01-landid-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "Blade Developer"
        }
    ],
    "require": {
        "php": "~8.2.0|~8.3.0",
        "blade-ui-kit/blade-icons": "^1.4",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ITCareTCR\\LandID\\Icons": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ITCareTCR\\LandID\\Icons\\LandIDIconServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
